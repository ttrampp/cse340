<h1><%= title %></h1>
<%- messages() %>

<form action="/account/login" method="post" class="form">
    <label for="accountEmail">Email:</label>
    <input type="email" name="accountEmail" id="accountEmail" required>

    <label for="accountPassword">Password:</label>
    <input type="password" name="accountPassword" id="accountPassword" required>

    <button type="submit">Login</button>
</form>

<p>Don't have an account? <a href="/account/register">Register</a></p>
